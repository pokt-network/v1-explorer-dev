{
  "name": "pokt-v1-explorer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "db-pull": "prisma db pull",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "pnpm prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "prebuild": "true",
    "disabled-prebuild": "run-s generate migrate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "migrate-dev": "true",
    "migrate": "prisma migrate deploy",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "pnpm generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.18.1",
    "@nextui-org/button": "0.0.0-dev-v2-20230718133109",
    "@nextui-org/card": "0.0.0-dev-v2-20230718133109",
    "@nextui-org/input": "0.0.0-dev-v2-20230718133109",
    "@nextui-org/link": "0.0.0-dev-v2-20230718133109",
    "@nextui-org/navbar": "0.0.0-dev-v2-20230718133109",
    "@nextui-org/react": "0.0.0-dev-v2-20230718133109",
    "@nextui-org/table": "0.0.0-dev-v2-20230718133109",
    "@nextui-org/tabs": "0.0.0-dev-v2-20230718133109",
    "@nextui-org/system": "0.0.0-dev-v2-20230718133109",
    "@prisma/client": "^4.14.1",
    "@react-aria/ssr": "^3.7.0",
    "@react-aria/visually-hidden": "^3.8.2",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.34.0",
    "@trpc/next": "^10.27.3",
    "@trpc/react-query": "^10.27.3",
    "@trpc/server": "^10.34.0",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.7",
    "framer-motion": "^10.12.22",
    "jotai": "^2.2.0",
    "next": "^13.4.3",
    "postcss": "^8.4.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.7.4",
    "tailwindcss": "^3.3.3",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.10",
    "@playwright/test": "^1.26.1",
    "@types/node": "^18.7.20",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript-codegen": "^0.24.0",
    "prettier": "^2.8.8",
    "prisma": "^4.14.1",
    "start-server-and-test": "^1.12.0",
    "tsx": "^3.12.7",
    "typescript": "^4.8.3",
    "vite": "^4.1.2",
    "vitest": "^0.28.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
